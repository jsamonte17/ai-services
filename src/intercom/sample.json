          {
            "type": "conversation_part",
            "id": "337",
            "part_type": "open",
            "body": "<p>Can you tell me about your services, because I am currently testing your api response.</p>",
            "created_at": 1719565769,
            "updated_at": 1719565769,
            "notified_at": 1719565769,
            "assigned_to": null,
            "author": {
